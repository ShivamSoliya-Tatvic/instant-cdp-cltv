{   
    "$schema": "http://json-schema.org/draft-2020-12/schema#",
    "$id": "https://example.com/leadsquared-to-unified-schema-mapping.schema.json",
    "title": "LeadSquared to Unified Schema Mapping",
    "description": "This schema defines the mapping of fields between the LeadSquared schema and the Unified Schema.",
    "mapping": [
      {
        "leadsquared_schema": "mx_Application_Number",
        "unified_schema": "lead_id"
      },
      {
        "leadsquared_schema": "ProspectStage",
        "unified_schema": "lead_disposition_status"
      },
      {
        "leadsquared_schema": "FirstName",
        "unified_schema": "first_name"
      },
      {
        "leadsquared_schema": "LastName",
        "unified_schema": "last_name"
      },
      {
        "leadsquared_schema": "Mobile",
        "unified_schema": "mobile_number"
      },
      {
        "leadsquared_schema": "EmailAddress",
        "unified_schema": "email_id"
      },
      {
        "leadsquared_schema": "LeadAge",
        "unified_schema": "age"
      },
      {
        "leadsquared_schema": "mx_Date_of_Birth",
        "unified_schema": "date_of_birth"
      }
    ]
  }
  